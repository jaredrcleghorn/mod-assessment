{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "Patron Group",
  "description": "A patron group, with counts",
  "type": "object",
  "properties": {
    "name": {
      "description": "The name of the patron group",
      "type": "string"
    },
    "counts": {
      "description": "The counts, broken down by library",
      "type": "object"
    }
  },
  "additionalProperties": false,
  "required": [ "name", "counts" ]
}
